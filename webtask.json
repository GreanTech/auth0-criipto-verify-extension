{
  "title": " Criipto Verify Administration",
  "name": "auth0-criipto-verify-admin",
  "version": "1.0.0",
  "author": "auth0",
  "useHashName": false,
  "description": "This extension manages connections to various national e-ID schemes via Criipto Verify.",
  "type": "application",
  "logoUrl": "https://www.criipto.com/images/logo-criipto-dark-3.svg",
  "initialUrlPath": "/admins/login",
  "repository": "https://github.com/GreanTech/auth0-criipto-verify-extension",
  "keywords": [
    "auth0",
    "extension",
    "criipto",
    "verify",
    "nemid",
    "bankid",
    "digid"
  ],
  "auth0": {
    "createClient": true,
    "scopes": "read:connections create:connections"
  },
  "criipto" : {
    "verify" : {
      "idServiceDisplayName" : "Danish NemID",
      "acrValues" : [
        "urn:grn:authn:dk:nemid:poces",
        "urn:grn:authn:dk:nemid:moces",
        "urn:grn:authn:dk:nemid:moces:codefile"      
      ]
    }
  }
}
